    <!--Nav Bar CSS Code-->
    /* Add a black background color to the top navigation 
.topnav {
  background-color: rgba(56, 56, 148, 0.959);
  overflow: hidden;
}

/* Style the links inside the navigation bar 
.topnav a {
  float: left;
  color: rgba(0, 0, 0, 0.733);
  background-color: rgba(63, 63, 168, 0.959);
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}

/* Change the color of links on hover 
.topnav a:hover {
  background-color: rgba(9, 27, 184, 0.178);
  color: black;
}

/* Add a color to the active/current link 
.topnav a.active {
  background-color: rgba(9, 27, 184, 0.178);
  color: rgba(0, 0, 0, 0.733);
}*/